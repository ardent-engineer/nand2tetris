/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    	Nand(a = a, b = a, out = Nota);
	Nand(a = b, b = a, out = Notb);
	And(a = a, b = Notb, out = aAndNotb);
	And(a = b, b = Nota, out = bAndNota);
	Or(a = aAndNotb, b = bAndNota, out = out);
}